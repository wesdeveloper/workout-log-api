{
  "compilerOptions": {
    "target": "es5",
    "moduleResolution": "node",
    "module": "commonjs",
    "sourceMap": true,
    "outDir": "dist",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "noImplicitThis": true,
    "forceConsistentCasingInFileNames": true,
    "baseUrl": "."
  },
  "exclude": [
    "node_modules",
    "dist"
  ]
}
