{
  "env": {
    "jest": true
  },
  "extends": [
    "airbnb-typescript/base"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "sourceType": "module",
    "project": "./tsconfig.json"
  },
  "plugins": [
    "@typescript-eslint"
  ],
  "rules": {
    "import/extensions": 0,
    "class-methods-use-this": 0,
    "import/prefer-default-export": 0,
    "max-len": 0
  }
}
